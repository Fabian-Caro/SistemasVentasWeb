[32m+[m[32m    public EmpleadoDTO validar(String user, String dni) {[m
[32m+[m
[32m+[m[32m        EmpleadoDTO em = new EmpleadoDTO();[m
[32m+[m
[32m+[m[32m        String sql = "SELECT * FROM empleado WHERE User=? AND Dni=?";[m
[32m+[m
[32m+[m[32m        try {[m
[32m+[m
[32m+[m[32m            con = conexion.Conexion();[m
[32m+[m
[32m+[m[32m            pst = con.prepareStatement(sql);[m
[32m+[m
[32m+[m[32m            pst.setString(1, user);[m
[32m+[m
[32m+[m[32m            pst.setString(2, dni);[m
[32m+[m
[32m+[m[32m            rs = pst.executeQuery();[m
[32m+[m
[32m+[m[32m            while (rs.next()) {[m
[32m+[m
[32m+[m[32m                em.setIdEmpleado(rs.getInt("IdEmpleado"));[m
[32m+[m
[32m+[m[32m                em.setUser(rs.getString("User"));[m
[32m+[m
[32m+[m[32m                em.setDni(rs.getString("Dni"));[m
[32m+[m
[32m+[m[32m                em.setNombre(rs.getString("Nombres"));[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        } catch (Exception e) {[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return em;[m
[32m+[m[32m    }[m
